<h1> Here is our menu: </h1>

<h3> drink list : {{dataValues.drinklist}} </h3>
<ul id ="drink-list">
  
</ul>
<ul>
  {{#each dbMenu}}
    <li>
      <p>Name: {{dataValues.drink_name}}</p>
      <p>Price: {{dataValues.price}}</p>
      <p>Description: {{dataValues.description}}</p>
      <p>Ingredients: {{dataValues.ingredients}}</p>
      <p>Category: {{dataValues.category}}</p>
      <img src="{{dataValues.images}}">
      <br>
      <button data-name="{{dataValues.drink_name}}" data-price="{{dataValues.price}}" class="place-order">Order</button>
    </li>
  {{/each}}
</ul>

